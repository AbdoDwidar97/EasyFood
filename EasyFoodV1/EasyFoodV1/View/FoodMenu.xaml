<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EasyFoodV1.View.FoodMenu">
    <ContentPage.Content>
        <StackLayout Padding="14">

            <ListView x:Name="FoodMenuLst">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding Item_Name}" TextColor="Black"></TextCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>